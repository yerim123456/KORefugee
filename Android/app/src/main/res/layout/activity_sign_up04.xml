<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Login.Finish_Activity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_marginLeft="43dp"
        android:layout_marginTop="137dp"
        android:background="@drawable/finish"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:text="All sign up processes is completed.
        Press the start button to “RE”start your life!"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />


    <Button
        android:id="@+id/end"
        android:layout_width="320dp"
        android:layout_height="70dp"
        android:layout_marginTop="260dp"
        android:background="@drawable/roundedge_8_orange"
        android:text="Start"
        android:textSize="40dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />



</androidx.constraintlayout.widget.ConstraintLayout>